{
  "name": "stealthdrop",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "description": "A template repository to get started with writing zero knowledge programs with Noir.",
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "test": "ts-mocha --config ../../.mocharc.js --exit src/*.spec.ts",
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf dist node_modules",
    "artifacts:generate": "pcd-artifacts generate && cp artifacts/* ../../apps/passport-client/public/artifacts/zk-eddsa-event-ticket-pcd && cp artifacts/* ../../apps/passport-server/public/artifacts/zk-eddsa-event-ticket-pcd"
  },
  "dependencies": {
    "@aztec/bb.js": "^0.8.14",
    "@noir-lang/backend_barretenberg": "^0.7.10-37315f8.nightly",
    "@noir-lang/noir_js": "nightly",
    "@noir-lang/types": "nightly",
    "@pcd/util": "0.2.0",
    "@pcd/passport-ui": "0.8.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "1.0.0",
    "@nomicfoundation/hardhat-viem": "1.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "dotenv": "^16.0.3",
    "hardhat": "^2.18.1",
    "hardhat-gas-reporter": "^1.0.8",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.1",
    "solidity-coverage": "^0.8.1",
    "ts-node": "^10.9.1",
    "viem": "1.16.6",
    "wagmi": "^1.4.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.26",
    "chai": "^4.3.10",
    "ethers": "^5.4.7",
    "typescript": "^5.0.0"
  }
}
